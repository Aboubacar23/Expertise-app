                                                        <td>{{ parametre.autreControle.balaisMassePreconisation6 }}</td>
                                                        <td>{{ parametre.autreControle.balaisMasseConformite6 }}</td>
                                                    </tr>
                                                </table>                                                    
                                            {% endif %}                                  
                                        </div>
                                        <!-- /fin Autres contôles -->

                                        <!-- début photo -->
                                        <div class="row">
                                            {% if parametre.photo %}
                                                {% for item in parametre.photo.images %}
                                                    <div class="col-md-2">
                                                        <span class="image"><img class="p-2" src="{{asset('/photo_expertises/'~ item.libelle)}}" height="150" width="150"> </span>
                                                    </div>
                                                {% endfor %}                                                
                                            {% endif %}
                                        </div>
                                        <!-- /fin photo -->

                                        <!-- début constats eléc -->
                                        <div class="row">
                                            <table class="table table-bordered table-striped">
                                                <tr class="table-primary">
                                                    <th colspan="6">Photos</th>
                                                </tr>
                                                <tr class="table-primary">
                                                    <th>Vérification</th>
                                                    <th>Critères</th>
                                                    <th>Observation</th>
                                                    <th>Préconisations et conclusion</th>
                                                    <th>Retenu</th>
                                                    <th>Photo</th>
                                                </tr>
                                                {% for item in parametre.constatElectriques %}
                                                    <tr class="table-striped">
                                                        <td>{{item.verification}}</td>
                                                        <td>{{item.critere}}</td>
                                                        <td>{{item.observation}}</td>
                                                        <td>{{item.preconisationConclusion}}</td>
                                                        <td>{{item.retenu}}</td>
                                                        <td class="text-center image"><img src="{{asset('/photo_constat_electrique/'~ item.photo)}}" alt="" height="75"></td>
                                                    </tr>                                                    
                                                {% endfor %}
                                            </table>
                                        </div>
                                        <!-- /fin constats eléc -->

                                    </div>
                                </div>
                            </section>
                        </div> 
                    </div>
                </div>
            </div>
    </div>
{% endblock %}
