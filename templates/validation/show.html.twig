                                                    <td>{{item.pjoule }}</td>
                                                </tr>
                                            {% endfor %}
                                        {% endif %}
                                        </table>
                                    </div>
                                    {% if parametre.autrePointFonctionnementRotor %}
                                        <div class="row">
                                            <table class="table table-bordered table-striped">
                                                <tr class="text-center">
                                                    <th class="table-primary">M</th>
                                                    <th class="table-primary">Indication du peson</th>
                                                    <td>{{ parametre.autrePointFonctionnementRotor.m}}</td>
                                                    <td>kg</td>
                                                    <td></td>
                                                </tr>
                                                <tr class="text-center">
                                                    <th class="table-primary">ID</th>
                                                    <th class="table-primary">Courant de démarrage à Un</th>
                                                    <td>{{ parametre.autrePointFonctionnementRotor.iden}}</td>
                                                    <td>A</td>
                                                    <th>ID = Imoy * Un/U</th>
                                                </tr>
                                                <tr class="text-center">
                                                    <th class="table-primary">CD</th>
                                                    <th class="table-primary">Couple de démarrage à un</th>
                                                    <td>{{ parametre.autrePointFonctionnementRotor.cd}}</td>
                                                    <td>Nm</td>
                                                    <th>CD = (9.81 * M * Un/U)</th>
                                                </tr>
                                            </table>
                                        </div>
                                    {% endif %}
                                     <!-- fin point fonctionnement rotor --> 
               
                                    <!-- début Appareils de mesure utilisés -->
                                    <div class="row">
                                        {% if parametre.appareilMesures %}
                                            <table class="table table-bordered">
                                                <tr class="table-primary">
                                                    <th>Désignation du moyen de contrôle</th>
                                                    <th>N° de l'appareil</th>
                                                    <th>Date de validité</th>
                                                </tr>
                                                {% for item in parametre.appareilMesureElectriques %}
                                                    <tr>
                                                        <td>{{item.appareil.designation}}</td>
                                                        <td>{{item.appareil.numAppareil}}</td>
                                                        <td>{{item.appareil.dateValidite | date('Y-m-d')}}</td>
                                                    </tr>
                                                {% endfor %} 
                                            </table>                                        
                                        {% endif %}
                                    </div>
                                    <!-- /fin Appareils de mesure utilisés -->
                                    
                                    <!-- début constats eléc après lavage -->
                                    <div class="row">
                                        <table class="table table-bordered">
                                            <tr class="table-primary">
                                                <th>Vérification</th>
                                                <th>Critères</th>
                                                <th>Observation</th>
                                                <th>Préconisations et conclusion</th>
                                                <th>Retenu</th>
                                                <th>Photo</th>
                                            </tr>
                                            {% for item in parametre.constatElectriqueApresLavages %}
                                                <tr class="table-striped">
                                                    <td>{{item.verification}}</td>
                                                    <td>{{item.critere}}</td>
                                                    <td>{{item.observation}}</td>
                                                    <td>{{item.preconisationConclusion}}</td>
                                                    <td>{{item.retenu}}</td>
                                                    <td class="text-center image"><img src="{{asset('/photo_constat_electrique_apres_lavage/'~ item.photo)}}" alt="" height="75"></td>
                                                </tr>                                                    
                                            {% endfor %}
                                        </table>
                                    </div>
                                    <!-- /fin constats eléc après lavage -->
                                </div>
                            </div>
                        </div> 
                    </div>
                {# fin expertise électrique après lavage #}
                
                <!-- début bouton -->
                <div class="x_panel"> 
                    <div class="x_content">
                        <div class="row">
                            <div class="col-md-12 text-center">
                                <a href="{{path('app_validation_valide', {'id' : parametre.id })}}" class="btn btn-danger btn-sm text-uppercase" onclick="confirm('Voulez-vous vraiment valider ?')">Validation Finale<i class="fa fa-check-circle p-2"></i></a>
                            </div>
                        </div> 
                    </div>
                </div>
            <!-- /fin bouton -->
            </div>
        </div>
    </div>
{% endblock %}
