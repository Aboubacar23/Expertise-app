{% extends 'base.html.twig' %}

{% block title %}Affaire{% endblock %}

{% block body %}
    <div class="">
        <div class="page-title">
            <div class="title_left"><h3>Vérification</h3>
            </div>
        </div>
        <div class="clearfix"></div>
            <div class="row">
                <div class="col-md-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h6>
                                <strong class="pull-left"> {{ parametre.type }} :  <td>{{parametre.machine }}</strong>
                            </h1>
                            <ul class="nav navbar-right panel_toolbox">
                                <a href="#" onclick="history.back();" class="btn btn-secondary btn-sm p-2">Retour<i class="fa fa-arrow-circle-left p-1"></i></a>
                            </ul>
                            <div class="clearfix"></div> 
                        </div>
                    </div>
                </div>
            </div>  
        </div>
        <div class="row">
            <div class="col-md-12 col-sm-12 ">
                {# début expertise électrique avant lavage #}
                    <div class="x_panel">
                        <div class="x_title">
                            <div class="row">
                                <div class="col-md-6"><h2>Expertise Électrique Avant Lavage</h2></div>
                                <div class="col-md-6">
                                    <ul class="nav navbar-right panel_toolbox">
                                        <li ><a class="collapse-link" data-toggle="collapse" data-target="#collapse1" aria-expanded="false" href="#collapse1"><i class="fa fa-chevron-up"></i></a></li>
                                    </ul>                                                
                                </div>
                            </div>
                            <div class="clearfix"></div> 
                        </div>
                        <div class="x_content collapse" id="collapse1">
                            <div class="row">
                                <div class="col-md-12">
                                    <!-- début contrôle visuel et recensement --> 
                                        <div class="row">
                                            <div class="col-md-12">
                                                <table class="table table-striped table-bordered">
                                                    <thead class="table-secondary">
                                                        <tr>
                                                            <th class="text-center" colspan="5">Contrôle visuel et recensement</th>
                                                        </tr>
                                                        <tr>
                                                            <th></th>
                                                            <th>Balais</th>
                                                            <th>Balais de masse</th>
                                                            <th class="text-center" colspan="2">Fac-similé boîte à borne</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        {% if parametre.controleVisuelElectrique %}
                                                            <tr>
                                                                <td>Dimension</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisDimension }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseDimension }}</td>
                                                                <td>Sens rotation</td>
                                                                <td>{{ parametre.controleVisuelElectrique.sensRotation }}</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Marque</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMarque }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseMarque }}</td>
                                                                <td>Remarque</td>
                                                                <td>{{ parametre.controleVisuelElectrique.remarque }}</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Quantité</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisQuantite }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseQuantite }}</td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                            <tr>
                                                                <td>Nuance</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisNuance }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseNuance }}</td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                            <tr>
                                                                <td>Longueur Shunt</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisLongueurShunt }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseLongueurShunt }}</td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                            <tr>
                                                                <td>Type de Cosse</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisTypeCosse }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseTypeCosse }}</td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                            <tr>
                                                                <td>Aspect de la patine</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisAspect }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseAspect }}</td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                            <tr>
                                                                <td>Type pression</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisTypePression }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMasseTypePression }}</td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                            <tr>
                                                                <td>Présence de Gaine</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisPresenceGaine ? 'Oui' : 'Non' }}</td>
                                                                <td>{{ parametre.controleVisuelElectrique.balaisMassePresenceGaine  ? 'Oui' : 'Non' }}</td>
                                                                <td></td>
                                                                <td></td>
                                                            </tr>
                                                        {% endif %}
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div> 
                                    <!-- fin contrôle visuel et recensement --> 

                                    <!-- fin Mesure d'isolement et resistance -->
                                        {% if parametre.mesureIsolement %}
                                            <div class="row">
                                                <table class="table table-bordered table-striped">
                                                    <tr  class="table-secondary">
                                                        <th colspan="6" class="text-center">Paramètres</th>
                                                    </tr>
                                                    <tr>
                                                        <th>T° Ambiante (°C)</th>
                                                        <th>T° Tôlerie (°C)</th>
                                                        <th>Hygrométrie (%)</th>
                                                        <th>Date d'essai</th>
                                                        <th>Critère</th>
                                                    </tr>
                                                    <tr>
                                                        <td>{{ parametre.mesureIsolement.tempAmbiante}}</td>
                                                        <td >{{ parametre.mesureIsolement.tempTolerie}}</td>
                                                        <td >{{ parametre.mesureIsolement.dateEssais ? parametre.mesureIsolement.dateEssais| date('d-m-Y') }}</td>
                                                        <td >{{ parametre.mesureIsolement.hygrometrie}}</td>
                                                        <td>{{ parametre.critere}} Ω/V </td>
                                                    </tr>   
                                                </table>
                                            </div>
                                            <div class="row">
                                                <table class="table table-bordered table-striped">
                                                    <tr class="table-secondary">
                                                        <th width="16%">Contrôle</th>
                                                        <th width="16%">Critère</th>
                                                        <th width="16%">Tension d'éssai</th>
                                                        <th width="16%">Valeur relevées</th>
                                                        <th width="16%">T.Correction à 20°</th>
                                                        <th width="16%">Conformité</th>
                                                    </tr>
                                                    {% for item in parametre.mesureIsolement.lmesureIsolements %}
                                                        <tr>
                                                            <td>{{item.controle}}</td>
                                                            <td>{{item.critere}}</td>
                                                            <td>{{item.tension}}</td>
                                                            <td>{{item.valeur}} {{item.unite}}</td>
                                                            <td>{{item.tempCorrection}}</td>
                                                            <td>{{item.conformite}}</td>
                                                        </tr>                                        
                                                    {% endfor %}
                                                </table>
                                            </div>
                                        {% endif %}
                                    <!-- fin Mesure d'isolement et resistance -->

                                    <!-- début Point de fonctionnement à vide -->                                           
                                        <div class="row">
                                            <table class="table table-bordered table-striped">
                                                <tr class="table-secondary">
                                                    <th class="text-center">Point de fonctionnement à vide</th>
                                                </tr>
                                                {% for item in parametre.pointFonctionnements %}
                                                    <tr>
                                                        <td class="text-center"><img src="{{asset('/photo_point_fonctionnement_vide/'~ item.image)}}" alt=""></td>
                                                    </tr>
                                                {% endfor %}
                                            </table>
                                        </div>
                                    <!-- fin Point de fonctionnement à vide -->

                                    <!-- début Mesures vibratoires -->
                                        <div class="row">
                                            {% if parametre.mesureVibratoire %}
                                                <table class="table table-bordered">
                                                    <tr class="table-secondary text-center">
                                                        <th colspan="5">Mesures Vibratoires</th>
                                                    </tr>
                                                    <tr class="table-secondary text-center">
                                                        <th>Position</th>
                                                        <th>Montage</th>
                                                        <th>Accouplement</th>
                                                        <th>Clavette</th>
                                                        <th>Commentaire</th>
                                                    </tr>
                                                    <tr>
                                                        <td>{{ parametre.mesureVibratoire.position}}</td>
                                                        <td>{{ parametre.mesureVibratoire.montage}}</td>
                                                        <td>{{ parametre.mesureVibratoire.accouplement}}</td>
                                                        <td>{{ parametre.mesureVibratoire.clavette}}</td>
                                                        <td>{{ parametre.mesureVibratoire.commentaire}}</td>
                                                    </tr>
                                                </table>
                                                <table class="table table-bordered">
                                                    <tr  class="table-secondary">
                                                        <th>Critère acceptation max 25mm/s</th>
                                                        <th>N (tr/min)</th>
                                                        <th>A</th>
                                                        <th>B</th>
                                                        <th>C</th>
                                                        <th>D</th>
                                                        <th>E</th>
                                                        <th>F</th>
                                                    </tr>
                                                    <tr>
                                                        <td>R.M.S de 10 à 1000 Hz à vide (mm/s)</td>
                                                        <td>{{ parametre.mesureVibratoire.n10 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.a10 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.b10 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.c10 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.d10 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.e10 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.f10 | number_format(2) }}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>R.M.S à F0 (mm/s)</td>
                                                        <td>{{ parametre.mesureVibratoire.n25 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.a25 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.b25 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.c25 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.d25 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.e25 | number_format(2) }}</td>
                                                        <td>{{ parametre.mesureVibratoire.f25 | number_format(2) }}</td>
                                                    </tr>
                                                    {% if parametre.mesureVibratoire.lmesureVibratoires %}
                                                        {% for item in parametre.mesureVibratoire.lmesureVibratoires %}
                                                            <tr>
                                                                <td class="titre_table">{{ item.titre}}</td>
                                                                <td>{{ item.n30 | number_format(2) }}</td>
                                                                <td>{{ item.a30 | number_format(2) }}</td>
                                                                <td>{{ item.b30 | number_format(2) }}</td>
                                                                <td>{{ item.c30 | number_format(2) }}</td>
                                                                <td>{{ item.d30 | number_format(2) }}</td>
                                                                <td>{{ item.e30 | number_format(2) }}</td>
                                                                <td>{{ item.f30 | number_format(2) }}</td>
                                                            </tr>
                                                        {% endfor %}                                        
                                                    {% endif %}
                                                    <tr>
                                                        <td>Observations</td>
                                                        <td colspan="7">{{ parametre.mesureVibratoire.obervation}}</td>
                                                    </tr>
                                                </table>

                                            {% endif %}
                                        </div>
                                    <!-- fin Mesures vibratoires -->

                                    <!-- début Appareils de mesure utilisés -->
                                        <div class="row">
                                            {% if parametre.appareilMesures %}
                                                <table class="table table-bordered table-striped">
                                                    <tr  class="table-secondary">
                                                        <th colspan="3" class="text-center">Appareils de mesure utilisés</th>
                                                    </tr>
                                                    <tr  class="table-secondary">
                                                        <th>Désignation du moyen de contrôle</th>
                                                        <th>N° de l'appareil</th>
                                                        <th>Date de validité</th>
                                                    </tr>
                                                    {% for item in parametre.appareilMesures %}
                                                        <tr>
                                                            <td>{{item.appareil.designation}}</td>
                                                            <td>{{item.appareil.numAppareil}}</td>
                                                            <td>{{item.appareil.dateValidite | date('d-m-Y')}}</td>
                                                        </tr>
                                                    {% endfor %} 
                                                </table>                                        
                                            {% endif %}
                                        </div>
                                    <!-- /fin Appareils de mesure utilisés -->

                                    <!-- début Contrôles de bobinages -->
                                        <div class="row">
                                            <table class="table table-bordered table-striped">
                                                <tr  class="table-secondary">
                                                    <th class="text-center" colspan="3">Contrôles de bobinages</th>
                                                </tr>
                                                <tr  class="table-secondary">
                                                    <th>Nature du contrôle</th>
                                                    <th>Conformité</th>
                                                    <th>Constats</th>
                                                </tr>
                                                {% if parametre.controleBobinage %}
                                                    <tr>
                                                        <th class="table-secondary">Contrôle calage des encoches</th>
                                                        <td>{{ parametre.controleBobinage.conformite1}}</td>
                                                        <td>{{ parametre.controleBobinage.constat1}}</td>
                                                    </tr>
                                                    <tr>
                                                        <th class="table-secondary">Contrôle laçage des développantes</th>
                                                        <td>{{ parametre.controleBobinage.conformite2}}</td>
                                                        <td>{{ parametre.controleBobinage.constat2}}</td>
                                                    </tr>                                                    
                                                {% endif %}
                                            </table>
                                        </div>
                                    <!-- /fin Contrôles de bobinages -->
                                    
                                    <!-- début Autres contôles -->
                                        <div class="row">
                                            {% if parametre.autreControle %}>
                                                {% if parametre.presenceBalais == 1 %}
                                                    <table class="table table-bordered table-striped">
                                                        <tr  class="table-secondary">
                                                            <th colspan="4" class="text-center">Balais</th>
                                                        </tr>
                                                        <tr  class="table-secondary">
                                                            <th>Constats</th>
                                                            <th>Critères</th>
                                                            <th>Préconisation</th>
                                                            <th>Conformité</th>
                                                        </tr>
                                                        <tr>
                                                            <td>Distance porte-balais/bague</td>
                                                            <td>2,5 m à 3 mn</td>
                                                            <td>{{ parametre.autreControle.balaisPreconisation1 }}</td>
                                                            <td>{{ parametre.autreControle.balaisConformite1 }}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Pression des balais</td>
                                                            <td>À définir suivant le type de balais</td>
                                                            <td>{{ parametre.autreControle.balaisPreconisation2 }}</td>
                                                            <td>{{ parametre.autreControle.balaisConformite2 }}</td>
                                                        </tr>
                                                        {#<tr>
                                                            <td>Isolement jeu de bagues à réception</td>
                                                            <td>À définir suivant le type de balais</td>
                                                            <td>{{ parametre.autreControle.balaisPreconisation3 }}</td>
                                                            <td>{{ parametre.autreControle.balaisConformite3 }}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Isolement jeu de bagues après lavage</td>
                                                            <td>À définir suivant le type de balais</td>
                                                            <td>{{ parametre.autreControle.balaisPreconisation4 }}</td>
                                                            <td>{{ parametre.autreControle.balaisConformite4 }}</td>
                                                        </tr>#}
                                                        <tr>
                                                            <td>Rugosité du collecteur</td>
                                                            <td>Nuancier Mersen</td>
                                                            <td>{{ parametre.autreControle.balaisPreconisation5 }}</td>
                                                            <td>{{ parametre.autreControle.balaisConformite5 }}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Aspect des patines</td>
                                                            <td>Nuancier Mersen</td>
                                                            <td>{{ parametre.autreControle.balaisPreconisation6 }}</td>
                                                            <td>{{ parametre.autreControle.balaisConformite6 }}</td>
                                                        </tr>
                                                    </table>

                                                {% endif %}
                                                {% if parametre.presenceBalaisMasse == 1 %}
                                                    <table class="table table-bordered table-striped">
                                                        <tr  class="table-secondary">
                                                            <th colspan="4" class="text-center">Balais de masse</th>
                                                        </tr>
                                                        <tr  class="table-secondary">
                                                            <th>Constats</th>
                                                            <th>Critères</th>
                                                            <th>Préconisation</th>
                                                            <th>Conformité</th>
                                                        </tr>
                                                        <tr>
                                                            <td>Distance porte-balais/bague</td>
                                                            <td>2,5 m à 3 mn</td>
                                                            <td>{{ parametre.autreControle.balaisMassePreconisation1 }}</td>
                                                            <td>{{ parametre.autreControle.balaisMasseConformite1 }}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Pression des balais</td>
                                                            <td>À définir suivant le type de balais</td>
                                                            <td>{{ parametre.autreControle.balaisMassePreconisation2 }}</td>
                                                            <td>{{ parametre.autreControle.balaisMasseConformite2 }}</td>
                                                        </tr>
                                                       {# <tr>
                                                            <td>Isolement jeu de bagues à réception</td>
                                                            <td>À définir suivant le type de balais</td>
                                                            <td>{{ parametre.autreControle.balaisMassePreconisation3 }}</td>
                                                            <td>{{ parametre.autreControle.balaisMasseConformite3 }}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Isolement jeu de bagues après lavage</td>
                                                            <td>À définir suivant le type de balais</td>
                                                            <td>{{ parametre.autreControle.balaisMassePreconisation4 }}</td>
                                                            <td>{{ parametre.autreControle.balaisMasseConformite4 }}</td>
                                                        </tr>#}
                                                        <tr>
                                                            <td>Rugosité du collecteur</td>
                                                            <td>Nuancier Mersen</td>
                                                            <td>{{ parametre.autreControle.balaisMassePreconisation5 }}</td>
                                                            <td>{{ parametre.autreControle.balaisMasseConformite5 }}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>Aspect des patines</td>
                                                            <td>Nuancier Mersen</td>
                                                            <td>{{ parametre.autreControle.balaisMassePreconisation6 }}</td>
                                                            <td>{{ parametre.autreControle.balaisMasseConformite6 }}</td>
                                                        </tr>
                                                    </table>
                                                {% endif %}
                                            {% endif %}
                                        </div>
                                    <!-- /fin Autres contôles -->

                                    <!-- début photo -->
                                        <div class="row">
                                            {% if parametre.photo %}
                                                {% for item in parametre.photo.images %}
                                                    <div class="col-md-2">
                                                        <span class="image"><img class="p-2" src="{{asset('/photo_expertises/'~ item.libelle)}}" height="150" width="150"> </span>
                                                    </div>
                                                {% endfor %}                                                
                                            {% endif %}
                                        </div>
                                    <!-- /fin photo -->

                                    <!-- début constats eléc -->
                                        <div class="row">
                                            <table class="table table-bordered table-striped">
                                                <tr  class="table-secondary">
                                                    <th colspan="6">Photos</th>
                                                </tr>
                                                <tr  class="table-secondary">
                                                    <th>Vérification</th>
                                                    <th>Critères</th>
                                                    <th>Observation</th>
                                                    <th>Préconisations et conclusion</th>
                                                    <th>Retenu</th>
                                                    <th>Photo</th>
                                                </tr>
                                                {% for item in parametre.constatElectriques %}
                                                    <tr class="table-striped">
                                                        <td>{{item.verification}}</td>
                                                        <td>{{item.critere}}</td>
                                                        <td>{{item.observation}}</td>
                                                        <td>{{item.preconisationConclusion}}</td>
                                                        <td>{{item.retenu}}</td>
                                                        <td class="text-center image"><img src="{{asset('/photo_constat_electrique/'~ item.photo)}}" alt="" height="75"></td>
                                                    </tr>                                                    
                                                {% endfor %}
                                            </table>
                                        </div>
                                    <!-- /fin constats eléc -->

                                </div>
                            </div>
                        </div> 
                    </div>
                {# fin expertise électrique avant lavage #}

                {# début expertise Mécanique #}
                    <div class="x_panel">
                        <div class="x_title">
                            <div class="row">
                                <div class="col-md-6"><h2>Expertise Mécanique</h2></div>
                                <div class="col-md-6">
                                    <ul class="nav navbar-right panel_toolbox">
                                        <li ><a class="collapse-link" data-toggle="collapse" data-target="#collapse2" aria-expanded="true" href="#collapse2"><i class="fa fa-chevron-up"></i></a></li>
                                    </ul>                                                
                                </div>
                            </div>
                            <div class="clearfix"></div> 
                        </div>
                        <div class="x_content collapse" id="collapse2">
                            <div class="row">
                                <div class="col-md-12">
                                    <!-- début plaque --> 
                                        {% if parametre.plaques %}
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <table class="table table-striped table-bordered">
                                                        <tbody>
                                                            <tr>
                                                                {% for item in parametre.plaques %}
                                                                    <td><img src="{{asset('/photo_plauqe/' ~ item.photo)}}" height="200"/></td>
                                                                {% endfor %}
                                                            </tr>    
                                                                <tr>                                                
                                                                {% for item in parametre.plaques %}
                                                                    <td>{{item.libelle}}</td>
                                                                {% endfor %}
                                                            </tr>  
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div> 
                                        {% endif %}
                                    <!-- début plaque --> 

                                    <!-- début contrôle et recensement (photo à la réception )--> 
                                        {% if parametre.controleRecensements %}
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <table class="table table-striped table-bordered">
                                                        <tbody>
                                                            <tr>
                                                                {% for item in parametre.controleRecensements %}
                                                                    <td><img src="{{asset('/photo_controle_recensement/' ~ item.photo)}}" height="200"/></td>
                                                                {% endfor %}
                                                            </tr>    
                                                                <tr>                                                
                                                                {% for item in parametre.controleRecensements %}
                                                                    <td>{{item.libelle}}</td>
                                                                {% endfor %}
                                                            </tr>  
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div> 
                                        {% endif %}
                                    <!-- début contrôle et recensement --> 

                                    <!-- début contrôle visuel et recensement --> 
                                        {% if parametre.controleVisuelMecanique %}
                                            <div class="row">
                                                <div class="col-md-7">
                                                    <table class="table table-striped table-bordered">
                                                        <thead class="table-secondary">
                                                            <tr>
                                                                <th class="text-center" colspan="6">Contrôle visuel et recensement</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr>
                                                                <td>Bridage</td>
