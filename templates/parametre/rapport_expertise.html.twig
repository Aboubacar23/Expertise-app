    margin-left: auto;
    margin-right: auto;
    padding-left: 10px;
    padding-top: 10px;
    padding-right: 10px;
    }

    .card2 {
    width: 360px;
    height: 270px;
    border: 2px solid #675E49;
    margin-left: auto;
    margin-right: auto;
    padding-bottom: 15px;
    }

    .card-content {
    margin-top: 10px;
    }
    .card-content2 {
        margin-top: 20px;
        margin-left: 20px;
        text-align: left !important;
    }

    .card-content h3 {
    font-size: 28px;
    margin: 0;
    }

    .card-content p {
    font-size: 14px;
    margin: 5px 0;
    }

    .table_matiere{
        padding-top: 200px;
    }

    .page_break { page-break-before: always; }

    .table_sous_page1, .table_sous_page5_6_1, .table_sous_page5_7_1{
        width: 50%;
