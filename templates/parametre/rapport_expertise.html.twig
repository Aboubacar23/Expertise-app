        <div class="sous_page6_7">
            <h4 id="lien66">F. <span class="span_titre">Illustrations</span></h4>
            {% if parametre.photo %}
            <table>
                <tr class="titre_table">
                    <th>Illustrations</th>
                </tr>
                {% for item in parametre.photo.images %}
                <tr>
                    <td height="15%" class="td_image">
                        {% if item.libelle %}
                        <img src="{{ absolute_url(asset('photo_expertises/' ~ item.libelle)) }}" alt="{{ item.photo }}"
                            height="300px" width="300px">
                        {% endif %}
                    </td>
                </tr>
                <tr class="titre_table">
                    <th>{{item.lig }}</th>
                </tr>
                {% endfor %}
            </table>
            {% endif %}
        </div>
        <div class="sous_page6_7">
            <h4 id="lien67">G. <span class="span_titre">Observations, Préconisations et Conclusions ECE</span></h4>
            {% if parametre.constatElectriques %}
            <table>
                <tr class="titre_table">
                    <th>Vérification</th>
                    <th>Critères</th>
                    <th>Observation</th>
                    <th>Préconisations et conclusion</th>
                    <th>Retenu</th>
                    <th>Photo</th>
                </tr>
                {% for item in parametre.constatElectriques %}
                <tr>
                    <td>{{item.verification}}</td>
                    <td>{{item.critere}}</td>
                    <td>{{item.observation}}</td>
                    <td>{{item.preconisationConclusion}}</td>
                    <td>{{item.retenu}}</td>
                    {% if item.photo %}
                    <td height="20%" class="td_image"><img
                            src="{{ absolute_url(asset('photo_constat_electrique/' ~ item.photo)) }}"
                            alt="{{ item.photo }}" height="150px" width="150px"></td>
                    {% else %}
                    <td></td>
                    {% endif %}
                </tr>
                {% endfor %}
            </table>
            {% endif %}
        </div>
    </div>
    {# fin de la partie 6 #}

    {# la partie 7 expertise electrique après lavage #}
    <div class="page_break"></div>
    <div class="page6">
        <h2 id="lien7">VII. Expertise électrique après lavage</h2>
        <div class="sous_page6_1">
            <h4 id="lien71">A. <span class="span_titre">Essais plate-forme</span></h4>
            <h5 id="lien711">1. <span class="span_titre">Mesure d'isolement</span></h5>
            {% if parametre.statorApresLavage %}
            <table>
                <tr class="titre_table">
                    <th colspan="4" class="text-center">Paramètres</th>
                </tr>
                <tr>
                    <th>T° Ambiante (°C)</th>
                    <th>T° Tôlerie (°C)</th>
                    <th>Hygrométrie (%)</th>
                    <th>Critère</th>
                </tr>
                <tr>
                    <td>{{ parametre.statorApresLavage.tempAmbiante}}</td>
                    <td>{{ parametre.statorApresLavage.tempTolerie}}</td>
                    <td>{{ parametre.statorApresLavage.hygrometrie}}</td>
                    <td>{{ parametre.critere}} <span class="my_police"> Ω/V </span></td>
                </tr>
            </table>
            <table>
                <tr class="titre_table">
                    <th></th>
                    <th style="width: 150px;">Contrôle</th>
                    <th style="width: 60px;">Critère</th>
                    <th style="width: 70px;">Tension d'éssai(V)</th>
                    <th style="width: 70px;">Valeur relevées avant lavage</th>
                    <th style="width: 70px;">Valeur relevées après lavage</th>
                    <th style="width: 70px;">Correction à 20°</th>
                    <th style="width: 80px;">Conformité</th>
                </tr>
                {% for item in parametre.statorApresLavage.lstatorApresLavages %}
                <tr>
                    <td>{{item.type}}</td>
                    <td>{{item.controle}}</td>
                    <td>{{ item.critere }}</td>
                    <td>{{item.tensionEssai}}</td>
                    <td>
                        {% if item.controle == 'IP-UVW/masse' or item.controle == 'IP-U/VW+masse' or item.controle ==
                        'IP-V/UW+masse' or item.controle == 'IP-W/UV+masse' %}
                        {{item.valeurRelevee}}
                        {% else %}
                        {{item.valeurRelevee}} <span class="my_police">{{item.unite}}</span>
                        {% endif %}
                    </td>
                    <td>
                        {% if item.controle == 'IP-UVW/masse' or item.controle == 'IP-U/VW+masse' or item.controle ==
                        'IP-V/UW+masse' or item.controle == 'IP-W/UV+masse' %}
                        {{item.valeur}}
                        {% else %}
                        {{item.valeur}} <span class="my_police">{{item.unite}}</span>
                        {% endif %}
                    </td>
                    <td>{{item.tempCorrection}}</td>
                    <td>{{ item.conformite }}</td>
                </tr>
                {% endfor %}
            </table>
            {% endif %}

            <h5 id="lien712">2. <span class="span_titre">Mesure de Résistances</span></h5>
            {% if parametre.sondeBobinage %}
            <table>
                <tr class="titre_table">
                    <th colspan="4" class="text-center">Paramètres</th>
                </tr>
                <tr>
                    <th>T° Ambiante (°C)</th>
                    <th>T° Tôlerie (°C)</th>
                    <th>Hygrométrie (%)</th>
                    <th>Critère</th>
                </tr>
                <tr>
                    <td>{{parametre.sondeBobinage.tempAmbiante }}</td>
                    <td>{{parametre.sondeBobinage.tempTolerie }}</td>
                    <td>{{parametre.sondeBobinage.hygrometrie }}</td>
                    <td>+ ou - 5% de la valeur Rmoy</span></td>
                </tr>
            </table>
            <table>
                <tr class="titre_table">
                    <th></th>
                    <th width="16%">Contrôle</th>
                    <th width="10%">Critère <span class="my_police">(MΩ) <span></span></th>
                    <th width="16%">Valeur relevée avant lavage</th>
                    <th width="16%">Valeur relevées après lavage</th>
                    <th width="16%">Correction à 20°</th>
                    <th width="16%">Conformité</th>
                </tr>
                {% for item in parametre.sondeBobinage.lsondeBobinages %}
                <tr>
                    <td>{{item.type}}</td>
                    <td>{{item.controle}}</td>
                    <td style="text-align: center;">{{item.critere}}</td>
                    <td style="text-align: center;">{{item.valeurRelevee}}</td>
                    <td style="text-align: center;">{{item.valeur}} <span class="my_police">{{ item.unite }}</span></td>
                    <td style="text-align: center;">{{item.tempCorrection}}</td>
                    <td style="text-align: center;">{{item.conformite}}</td>
                </tr>
                {% endfor %}
            </table>
            {% endif %}

            {% if parametre.equirepartitions |length > 0 %}
            <h5 id="lien712">3. <span class="span_titre">Mesure d'équi-répartition</span></h5>
            <table>
                <tr>
                    <th colspan="3" class="titre_table">Equi-Répartition de tension Roue polaire</th>
                </tr>
                {% for item in parametre.equirepartitions %}
                <tr>
                    <th class="titre_table" style="text-align: left;">{{item.pole}}</th>
                    <td>{{item.tension}} V</td>
                    <td>{{item.conforme}}</td>
                </tr>
                {% endfor %}
            </table>
            {% endif %}

            {% if parametre.pontDiodes |length > 0 %}
            <h5 id="lien712">4. <span class="span_titre">Pont de Diodes</span></h5>
            <table>
                <tr>
                    <th colspan="3" class="titre_table">Pont de Diodes</th>
                </tr>
                {% for item in parametre.pontDiodes %}
                <tr>
                    <th class="titre_table" style="text-align: left;">{{item.diode}}</th>
                    <td>{{item.tension}} V</td>
                    <td>{{item.conforme}}</td>
                </tr>
                {% endfor %}
            </table>
            {% endif %}
        </div>
        <div class="sous_page6_4">
            {% if parametre.pontDiodes |length > 0 and parametre.equirepartitions |length > 0 %}
            <h5 id="lien713">5. <span class="span_titre">Appareils de mesure utilisés</span></h5>
            {% elseif parametre.pontDiodes |length > 0 or parametre.equirepartitions |length > 0 %}
            <h5 id="lien713">4. <span class="span_titre">Appareils de mesure utilisés</span></h5>
            {% else %}
            <h5 id="lien713">3. <span class="span_titre">Appareils de mesure utilisés</span></h5>
            {% endif %}

            {% if parametre.appareilMesureElectriques %}
            <table>
                <tr class="titre_table">
                    <th>Désignation du moyen de contrôle</th>
                    <th>N° de l'appareil</th>
                    <th>Date de validité</th>
                </tr>
                {% for item in parametre.appareilMesureElectriques %}
                <tr>
                    <td>{{item.appareil.designation}}</td>
                    <td>{{item.appareil.numAppareil}}</td>
                    <td>{{item.appareil.dateValidite | date('d-m-Y')}}</td>
                </tr>
                {% endfor %}
            </table>
            {% endif %}
        </div>
        <div class="sous_page6_7">
            <h4 id="lien72">B. <span class="span_titre">Observations, Préconisations et Conclusions ECE</span></h4>
            {% if parametre.constatElectriqueApresLavages %}
            <table>
                <tr class="titre_table">
                    <th>Vérification</th>
                    <th>Critères</th>
                    <th>Observation</th>
                    <th>Préconisations et conclusion</th>
                    <th>Retenu</th>
                    <th>Photo</th>
                </tr>
                {% for item in parametre.constatElectriqueApresLavages %}
                <tr>
                    <td>{{item.verification}}</td>
                    <td>{{item.critere}}</td>
                    <td>{{item.observation}}</td>
                    <td>{{item.preconisationConclusion}}</td>
                    <td>{{item.retenu}}</td>
                    {% if item.photo %}
                    <td height="20%" class="td_image"><img
                            src="{{ absolute_url(asset('photo_constat_electrique_apres_lavage/' ~ item.photo)) }}"
                            alt="{{ item.photo }}" height="150px" width="150px"></td>
                    {% else %}
                    <td></td>
                    {% endif %}
                </tr>
                {% endfor %}
            </table>
            {% endif %}
        </div>
    </div>
    {# fin de la partie 7 #}

    {# la partie 8 contre expertise #}
    {% if parametre.contreExpertise %}
    <div class="page_break"></div>
    <div class="page8">
        <h2 id="lien8">VIII. Contre-Expertise</h2>
        <p>{{ parametre.contreExpertise | raw }}</p>
    </div>
    {% endif %}
    {# fin de la partie 8 #}
</body>