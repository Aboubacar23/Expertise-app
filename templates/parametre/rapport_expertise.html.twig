    }
    
    .titre_table{
        background-color: #ECECCD;
    }

    .image_logo 
        {
        height: 100px;
    }
    .titre1{
        font-family: Arial, Helvetica, sans-serif;
        font-size: 50px;
        color: #675E49;
        font-weight: 200;
        border-left: 2px solid red;
    }

    .p_travaux{
        border-left: 2px solid red;
    }

    h3{
        color: azure;
    }
    h2,h5, h4{
        color:#675E49 ;
    }
    h2{
        color:#675E49 ;
        font-family: Arial, Helvetica, sans-serif;
        text-decoration: underline;
        font-style: normal;
    }
    h4{
        padding-left: 50px;
    }
    h5{
        padding-left: 100px;
    }

    .span_titre{
        color:#675E49 ;
        font-family: Arial, Helvetica, sans-serif;
        text-decoration: underline;
        font-style: normal;
    }

    .logo
    {
        text-align: right;
        height: 80px;
    }
    .titre_rapport{
        padding-top: 100px;
        text-align: center;
        margin-left: auto;
        margin-right: auto;

    }
    .card1 {
    width: 470px;
    height: 160px;
    border: 1px solid #675E49;
    background-color: #675E49;
