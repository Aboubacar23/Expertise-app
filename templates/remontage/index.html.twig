                            <div class="x_content">
                                <div class="row">
                                    <div class="col-md-12 text-center">
                                        <a href="{{path('valider_remontage', {'id' : parametre.id })}}" class="btn btn-danger btn-sm text-uppercase" onclick="return confirm('Voulez-vous vraiment valider ?')">Validation du Rapport final<i class="fa fa-check-circle p-2"></i></a>
                                    </div>
                                </div> 
                            </div>
                        </div>
                    {% endif %}
                <!-- /fin bouton -->
            </div>
        </div>                    
    <!-- /fin contenu du formulaire--> 
</div>
</div>
