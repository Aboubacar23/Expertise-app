								{% else %}
									<a href="{{path('app_info_generale', {'id' : parametre.id })}}" class="btn btn-outline-primary btn-sm">Info Générale</a>
								{% endif %}
							{% endif %}

						{% endif %}

						<ul class="nav navbar-right panel_toolbox">
