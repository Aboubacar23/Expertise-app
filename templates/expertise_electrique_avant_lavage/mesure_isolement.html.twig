                                        <td>{{ form_widget(form.temp_correction) }}</td>
                                        <td>{{ form_widget(form.conformite) }}</td>
                                        <td class="text-center"><button class="btn btn-primary btn-sm" name="bouton7" value="ajouter">Ajouter</button></td>
                                    </tr>
                                    {% for item2 in items %}
                                        <tr>
                                            <td>{{item2.type}}</td>
                                            <td>{{item2.controle}}</td>
                                            <td>
                                                {% if item2.controle == 'IP-UVW/masse' or  item2.controle == 'IP-U/VW+masse' or  item2.controle == 'IP-V/UW+masse' or  item2.controle == 'IP-W/UV+masse'  %}
                                                    >= {{item2.critere}}
                                                {% else %}
                                                        {{item2.critere}} MΩ
                                                {% endif %}
                                            </td>
                                            <td>{{item2.tension}}</td>
                                            <td colspan="2">
                                                {% if item2.controle == 'IP-UVW/masse' or  item2.controle == 'IP-U/VW+masse' or  item2.controle == 'IP-V/UW+masse' or  item2.controle == 'IP-W/UV+masse'  %}
                                                        {{item2.valeur}}
                                                {% else %}
                                                        {{item2.valeur}} {{ item2.unite}}
                                                {% endif %}
                                            </td>
                                            <td>
                                                {% if item2.controle == 'IP-UVW/masse' or  item2.controle == 'IP-U/VW+masse' or  item2.controle == 'IP-V/UW+masse' or  item2.controle == 'IP-W/UV+masse'  %}
                                                    -
                                                {% else %}
                                                    {{item2.tempCorrection}} {{ item2.unite}}
                                                {% endif %}
                                            </td>
                                            <td>{{item2.conformite}}</td>
                                            <td class="text-center"><a href="{{path('delete_mesure', {'id' : item2.lig, 'paramID' : parametre.id })}}"><i class="fa fa-trash-o fa-2x text-danger"></i></a></td>   
                                        </tr>                                        
                                    {% endfor %}
                                    <tr class="bg-primary"></tr>
                                    {% if parametre.mesureIsolement %}
                                        {% for item in parametre.mesureIsolement.lmesureIsolements %}
                                            <tr>
                                                <td>{{item.type}}</td>
                                                <td>{{item.controle}}</td>
                                                <td>
                                                    {% if item.controle == 'IP-UVW/masse' or   item.controle == 'IP-U/VW+masse' or  item.controle == 'IP-V/UW+masse' or  item.controle == 'IP-W/UV+masse'  %}
                                                        >= {{item.critere}} 
                                                    {% else %}
                                                            {{item.critere}}  M<span class="my_police">Ω</span>
                                                    {% endif %}
                                                </td>
                                                <td>{{item.tension}}</td>
                                                <td colspan="2">
                                                    {% if item.controle == 'IP-UVW/masse' or   item.controle == 'IP-U/VW+masse' or  item.controle == 'IP-V/UW+masse' or  item.controle == 'IP-W/UV+masse'  %}
                                                            {{item.valeur}}
                                                    {% else %}
                                                            {{item.valeur}} {{ item.unite}}
                                                    {% endif %}
                                                </td>
                                                <td>
                                                    {% if item.controle == 'IP-UVW/masse' or   item.controle == 'IP-U/VW+masse' or  item.controle == 'IP-V/UW+masse' or  item.controle == 'IP-W/UV+masse'  %}
                                                        -
                                                    {% else %}
                                                        {{item.tempCorrection}} {{ item.unite}}
                                                    {% endif %}
                                                </td>
                                                {% if item.conformite ==  'Conforme' %}
                                                    <td style="color: green;">{{item.conformite}}</td>
                                                {% else %}
                                                    <td style="color: red;">{{item.conformite}}</td>
                                                {% endif %}
                                                <td class="text-center">
                                                    <a class="btn btn-info btn-sm" href="{{path('app_edit_mesure_isolement', {'id' : item.id})}}"><i class="fa fa-edit fa-1x"></i></a>
