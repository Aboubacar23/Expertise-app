                                    <button class="btn btn-success btn-sm" name="bouton7" value="mesure_isolement_terminer"> Terminer <i class="fa fa-save"></i></button>
                                </div>
                            </div>
                        </div>
                    </form>
                <!-- fin Mesure d'isolement -->  
            </div>
        </div>
    </div> 
</div> 
