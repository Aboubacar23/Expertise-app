{% extends 'base.html.twig' %}

{% block title %}Affaire{% endblock %}

{% block body %}
<div class="">
    <div class="page-title">
        <div class="title_left">
            <h4>Expertise Électrique avant lavage du projet {{ parametre.affaire.numAffaire }}</h4>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">
                <div class="x_title">
                    <div class="row">
                        <div class="col-md-6">
                            <h5>Mesure d'isolement</h5>
                        </div>
                        <div class="col-md-6">
                            <ul class="nav navbar-right panel_toolbox">
                                <a href="{{path('app_expertise_electrique_avant_lavage',{'id' : parametre.id})}}" class="btn btn-secondary btn-sm p-2">Retour<i class="fa fa-arrow-circle-left p-1"></i></a>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
                    
                <!-- début Mesure d'isolement -->
                    <form method="post">
                        <div class="x_content">
                            <div class="row">
                                <table class="table table-bordered">
                                    <tr  class="table-secondary">
                                        <th colspan="9" class="text-center">Paramètres</th>
                                    </tr>
                                    <tr>
                                        <th width="15%">T° ambiante (°C)</th>
                                        <th width="15%">T° fer (°C)</th>
                                        <th width="15%">Hygrométrie (%)</th>
                                        <th width="15%">Date d'essai</th>
                                        <th width="10%">Critère</th>
                                        <th width="10%">
                                            {% if parametre.machine.categorie == "À courant continu" %}
                                                Tension Excitation
                                            {% else %}
                                                Tension Stator
                                            {% endif %}
                                        </th>
                                        <th width="10%">
                                            {% if parametre.machine.categorie == "À courant continu" %}
                                                Tension Excitation 2
                                            {% else %}
                                                Tension Stator 2
                                            {% endif %}
                                        </th>
                                        <th width="10%">Tension Rotor</th>
                                        <th width="10%">Tension Rotor 2</th>
                                    </tr>
                                    <tr>
                                        <td>{{ form_widget(formMesureIsolement.temp_ambiante) }}</td>
                                        <td >{{ form_widget(formMesureIsolement.temp_tolerie) }}</td>
                                        <td >{{ form_widget(formMesureIsolement.hygrometrie) }}</td>
                                        <td>{{ form_widget(formMesureIsolement.date_essais) }}</td>
                                        <td>{{parametre.critere}}</td>
                                        <td>
                                            {{parametre.statorTension}}
                                        </td>
                                        <td>
                                            {% if parametre.statorTension2 != 0 %}
                                                {{parametre.statorTension2 }}
                                            {% else %}
                                                0
                                            {% endif %}
                                        </td>
                                        <td>{{parametre.rotorTension}}</td>
                                        <td>
                                            {% if parametre.rotorTension2 != 0 %}
                                                {{parametre.rotorTension2 }}
                                                {% else %}
                                                0
                                            {% endif %}
                                        </td>
                                    </tr>   
                                </table>
                            </div>
                            <div class="row">
                                {% for flash_message in app.session.flashbag.get('message') %}
                                    <div class="container alert alert-danger p-2 ml-5 mr-5 mt-2" role="alert">
                                        {{ flash_message }}
                                    </div> 
                                {% endfor %}
                            </div>
                            <div class="row">
                                <table class="table table-bordered table-striped">
                                    <tr  class="table-secondary">
                                        <th width="13%">Type</th>
                                        <th width="13%">Contrôle</th>
                                        <th width="13%">Critère</th>
                                        <th width="13%">Tension d'éssai (V)</th>
                                        <th width="13%" colspan="2">Valeur relevées</th>
                                        <th width="13%">Correction à 20° C</th>
                                        <th width="13%">Conformité</th>
