{% extends 'base.html.twig' %}

{% block title %}Affaire{% endblock %}

{% block body %}
<div class="">
    <div class="page-title">
        <div class="title_left"><h4>Mesure d'isolement</h4>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">
                <div class="x_title">     
                    <ul class="nav navbar-right panel_toolbox">
                        <a href="{{path('app_expertise_electrique_apres_lavage',{'id' : parametre.id})}}" class="btn btn-secondary btn-sm p-2">Retour<i class="fa fa-arrow-circle-left p-1"></i></a>
                    </ul>
                    <div class="clearfix"></div> 
                </div> 
                <div class="x_content">
                    <form method="post">
                        <div class="x_content">
                            <div class="row">
                                <table class="table table-bordered">
                                    <tr  class="table-secondary">
                                        <th colspan="6" class="text-center">Paramètres</th>
                                    </tr>
                                    <tr>
                                        <th width="20%">T° Ambiante (°C)</th>
                                        <th width="20%">T° Tôlerie (°C)</th>
                                        <th width="20%">Hygrométrie (%)</th>
                                        <th width="10%">Critère</th>
                                        <th width="10%">Tension Rotor</th>
                                        <th width="10%">
                                            {% if parametre.machine.categorie == "À courant continu" %}
                                                Tension Excitation
                                            {% else %}
                                                Tension Stator
                                            {% endif %}
                                        </th>
                                    </tr>   
                                    <tr> 
                                        <td>{{ form_widget(formStatorApresLavage.temp_ambiante) }}</td>
                                        <td >{{ form_widget(formStatorApresLavage.temp_tolerie) }}</td>
                                        <td >{{ form_widget(formStatorApresLavage.hygrometrie) }}</td>
                                        <td>{{parametre.critere}} Ω/V </td>
                                        <td>{{parametre.rotorTension}}
                                            {% if parametre.rotorTension2 != 0 %}
                                                et {{parametre.rotorTension2 }}
                                            {% endif %}
                                        </td>
                                        <td>{{parametre.statorTension}} 
                                            {% if parametre.statorTension2 != 0 %}
                                                et {{parametre.statorTension2 }}
                                            {% endif %} 
                                        </td>
                                    </tr>  
                                </table> 
                            </div>
                            <div class="row">
                                {% for flash_message in app.session.flashbag.get('message') %}
                                    <div class="container alert alert-danger p-2 ml-5 mr-5 mt-2" role="alert">
                                        {{ flash_message }}
                                    </div>
                                {% endfor %}
                            </div>               
                            <div class="row">
                                <table class="table table-bordered table-striped">
                                    <tr  class="table-secondary">
                                        <th width="12%">Type</th>
                                        <th width="12%">Contrôle</th>
                                        <th width="12%">Critère</th>
                                        <th width="12%">Tension d'éssai (V)</th>
                                        <th width="8%">Valeur relevées avant Lavage</th>
                                        <th width="12%" colspan="2">Valeur relevées</th>
                                        <th width="12%">Correction à 20°</th>
                                        <th width="12%">Conformité</th>
                                        <th width="100"></th> 
                                    </tr>
                                    <tr>
                                        <td>{{ form_widget(form.type) }}</td>
                                        <td>{{ form_widget(form.controle) }}</td>
                                        <td>{{ form_widget(form.critere) }}</td>
                                        <td>{{ form_widget(form.tension_essai) }}</td>
                                        <td></td>
                                        <td>{{ form_widget(form.valeur) }}</td>
                                        <td width="200">{{form_widget(form.unite)}}</td>
                                        <td>{{ form_widget(form.temp_correction) }}</td>
                                        <td>{{ form_widget(form.conformite) }}</td>
