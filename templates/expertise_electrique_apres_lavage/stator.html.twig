                                            <td>{{item2.type}}</td>
                                            <td>{{item2.controle}}</td>
                                            <td>{{item2.critere}}</td>
                                            <td>{{item2.tensionEssai}}</td>
                                            <td>
                                                {% if item2.controle == 'IP-UVW/masse' or   item2.controle == 'IP-U/VW+masse' or  item2.controle == 'IP-V/UW+masse' or  item2.controle == 'IP-W/UV+masse'  %}
                                                    {{item2.valeurRelevee}}
                                                {% else %}
                                                    {{item2.valeurRelevee}} {{ item2.unite}}
                                                {% endif %}
                                            </td>
                                            <td colspan="2">
                                                {% if item2.controle == 'IP-UVW/masse' or   item2.controle == 'IP-U/VW+masse' or  item2.controle == 'IP-V/UW+masse' or  item2.controle == 'IP-W/UV+masse'  %}
                                                    {{item2.valeur}}
                                                {% else %}
                                                    {{item2.valeur}} {{ item2.unite}}
                                                {% endif %}
                                            </td>
                                            <td>{{item2.tempCorrection}}</td>
                                            <td>{{item2.conformite}}</td>
                                            <td class="text-center"><a href="{{path('delete_stator_session', {'id' : item2.lig, 'id2' : parametre.id })}}"><i class="fa fa-trash-o fa-2x text-danger"></i></a></td>   
                                        </tr>                                        
                                    {% endfor %} 
                                    <tr class="bg-primary"></tr>
