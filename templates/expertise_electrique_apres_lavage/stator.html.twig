                                                    {% else %}
                                                        {{item.valeurRelevee}} {{ item.unite}}
                                                    {% endif %}
                                                </td>
                                                <td colspan="2">
                                                    {% if item.controle == 'IP-UVW/masse' or  item.controle == 'IP-U/VW+masse' or  item.controle == 'IP-V/UW+masse' or  item.controle == 'IP-W/UV+masse'  %}
                                                        {{item.valeur}}
                                                    {% else %}
                                                        {{item.valeur}} {{ item.unite}}
                                                    {% endif %}
                                                </td>
                                                <td>{{item.tempCorrection | number_format(1)}}</td>
                                                <td>{{item.conformite}}</td>
                                                <td class="text-center"><a href="{{path('delete_stator_apres_session_lavage', {'id' : item.id, 'id2' : parametre.id })}}"><i class="fa fa-trash-o fa-2x text-danger"></i></a></td>   
                                            </tr>                                        
                                        {% endfor %}                                            
                                    {% endif %}
                                </table>
                            </div>
                            
