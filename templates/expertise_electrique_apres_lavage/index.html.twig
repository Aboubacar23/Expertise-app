                                                        <td class="text-left">{{item.controle}}</td>
                                                        <td class="text-center">{{ item.critere }}</td>
                                                        <td class="text-center">{{item.tensionEssai}}</td>
                                                        <td class="text-center">{{item.valeurRelevee}}</td>
                                                        <td class="text-center">{{ item.valeur}}</td>
                                                        <td class="text-center">{{ item.conformite }}</td>
                                                    </tr>                                                    
