            <!-- début constats eléc après lavage -->
            <div class="x_panel">
                <div class="x_title">
                    <div class="row">
                        <div class="col-md-4">
                            <h2>Constats Électrique Après Lavage</h2>
                        </div>
                        <div class="col-md-4 text-center">
                            {% if parametre.constatElectriqueApresLavages |length == 0 %}
                            <a href="#" class="btn btn-secondary btn-sm">En attente <i
                                    class="fa fa-plus-circle"></i></a>
                            {% else %}
                            {% if parametre.constatElectriqueApresLavages |length > 1 %}
                            <a href="#" class="btn btn-success btn-sm">Terminé <i class="fa fa-check"></i></a>
                            {% else %}
                            <a href="#" class="btn btn-danger btn-sm">En cours <i class="fa fa-circle-o"></i></a>
                            {% endif %}
                            {% endif %}
                        </div>
                        <div class="col-md-2 text-right">
                            {% if is_granted('ROLE_SUPER_ADMIN') or is_granted('ROLE_CHEF_PROJET') or
                            is_granted('ROLE_AGENT_MAITRISE') or is_granted('ROLE_VERIFICATEUR') or
                            is_granted('ROLE_TECHNICIEN_ELECTRICIEN') or is_granted('ROLE_TECHNICIEN_MECANICIEN') %}
                            {% if parametre.affaire.bloque != 1 %}
                            <a class="btn btn-primary"
                                href="{{path('app_Constat_expertise_apres_lavage', {'id' : parametre.id })}}"><i
                                    class="fa fa-plus-circle"></i></a>
                            {% endif %}
                            {% endif %}
                        </div>
                        <div class="col-md-2">
                            <ul class="nav navbar-right panel_toolbox">
                                <li><a class="collapse-link" data-toggle="collapse" aria-expanded="false"
                                        href="#collapse9"><i class="fa fa-chevron-up"></i></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content collapse" id="collapse9">
                    <div class="row">
                        <table class="table table-bordered">
                            <tr class="table-secondary">
                                <th>Vérification</th>
                                <th>Critère</th>
                                <th>Observation</th>
                                <th>Préconisations et conclusion</th>
                                <th>Retenu</th>
                                <th>Photo</th>
                            </tr>
                            {% for item in parametre.constatElectriqueApresLavages %}
                            <tr class="table-striped">
                                <td>{{item.verification}}</td>
                                <td>{{item.critere}}</td>
                                <td>{{item.observation}}</td>
                                <td>{{item.preconisationConclusion}}</td>
                                <td>{{item.retenu}}</td>
                                {% if item.photo %}
                                    <td class="text-center image"><img
                                        src="{{asset('/photo_constat_electrique_apres_lavage/'~ item.photo)}}" alt=""
                                        height="75"></td>
                                {% else %}
                                    <td></td>
                                {% endif %}
                            </tr>
                            {% endfor %}
                        </table>
                    </div>
                </div>
            </div>
            <!-- /fin constats eléc après lavage -->

            <!-- début bouton -->
            {% if is_granted('ROLE_SUPER_ADMIN') or is_granted('ROLE_CHEF_PROJET') or is_granted('ROLE_AGENT_MAITRISE')
            or is_granted('ROLE_VERIFICATEUR') or is_granted('ROLE_TECHNICIEN_ELECTRICIEN') %}
            <div class="x_panel">
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-12 text-center">
                            <a href="{{path('valider_expertise_electrique_apres_lavage', {'id' : parametre.id })}}"
                                class="btn btn-danger btn-sm text-uppercase"
                                onclick="return confirm('Voulez-vous vraiment valider ?')">Validation de l'expertise
                                électrique Après Lavage<i class="fa fa-check-circle p-2"></i></a>
                        </div>
                    </div>
                </div>
            </div>
            {% endif %}
            <!-- /fin bouton -->
        </div>
    </div>
    <!-- /fin contenu du formulaire-->
</div>
</div>
