    </div>
    {# fin sommaire #}

    {# la page 2 , Préambule et Contexte #}
        <div class="page_break"></div>
        <div class="page2">
            <div>
                <h2 id="lien1">I. Préambule et Contexte</h2>
                <p>{{parametre.affaire.context}}</p>

            </div>
            <div>
                <h2 id="lien2">II. Présentation des travaux</h2>
                <p>{{parametre.affaire.presentationTravaux | raw}}</p>
            </div>
            <div>
                {% if parametre.affaire.travauxSup %}
                <h2>II. Travaux Sup</h2>
                <p>{{parametre.affaire.travauxSup | raw}}</p>
                {% endif %}
            </div>
        </div>
    {# fin de la page 2 #}

    {# la page 3, Caractéristiques générales  #}
        <div class="page_break"></div>
        <div class="page3">
            <h2 id="lien3">III. Caractéristiques générales</h2>
            <div class="sous_page">
                {% if parametre.plaques %}
                <table>
                    <tr>
                        {% for item in parametre.plaques %}
                            <td height="15%" class="td_image">
                                {% if item.photo %}
                                <img src="{{ absolute_url(asset('photo_plaque/' ~ item.photo)) }}" alt="item.photo"
                                width="200px" height="200px">
                                {% endif %}
                            </td>
                        {% endfor %}
                    </tr>
                    <tr>
                        {% for item in parametre.plaques %}
                        <td>{{item.libelle}}</td>
                        {% endfor %}
                    </tr>
                </table>
                {% endif %}
            </div>
            <div class="sous_page1">
                <table>
                    <tr class="titre_table">
                        <th>Machine</th>
                        <th colspan="5">Type</th>
                    </tr>
                    <tr>
                        <td>{{parametre.typeMachine}}</td>
                        <td>{{parametre.typePalier}}</td>
                        <td>{{parametre.machine.categorie}}</td>
                        <td>{{parametre.machine.sousCategorie}}</td>
                        <td>{{parametre.machine.sousCategorie2}}</td>
                        <td>{{parametre.machine.sousCategorie3}}</td>
                    </tr>
                </table>
                <table>
                    <tr class="titre_table">
                        <th>Fabricant</th>
                        <th>Type</th>
                        <th>N° de Série</th>
                    </tr>
                    <tr>
                        <td>{{parametre.fabricant}}</td>
                        <td>{{parametre.type}}</td>
                        <td>
                            {% if parametre.controleVisuelMecanique %}{{parametre.controleVisuelMecanique.numeroSerie}}{% endif %}
                        </td>
                    </tr>
                </table>
                {% if parametre.lplaque %}
                <table>
                    <tr class="titre_table">
                        <th>Service</th>
                        <th>Classe Isolation</th>
                        <th>Indice de protection</th>
                    </tr>
                    <tr>
                        <td>{{ parametre.lplaque.service }}</td>
                        <td>{{ parametre.lplaque.classeIsolation }}</td>
                        <td>{{ parametre.lplaque.indiceProtection }}</td>
                    </tr>
                </table>
                {% endif %}
                <table>
                    <tr class="titre_table">
                        <th>Puissance</th>
                        <th>Vitesse</th>
                        <th>Montage</th>
                        <th>Masse</th>
                    </tr>
                    <tr>
                        <td>{{parametre.puissance}} KW</td>
                        <td>{{parametre.vitesse}} tr/min</td>
                        <td>{{parametre.montage}}</td>
                        <td>{{parametre.masse}} Kg</td>
                    </tr>
                </table>
            </div>
            <div class="sous_page2">
                <table class="table_sous_page1">
                    <tr class="titre_table">
                        {% if parametre.machine.categorie == "Asynchrone" %}
                            <th colspan="4">Stator</th>
                        {% elseif parametre.machine.categorie == "Synchrone" %}
                            <th colspan="4">Stator</th>
                        {% elseif parametre.machine.categorie == "À courant continu" %}
                            <th colspan="2">Caractèristiques électriques</th>
                        {% endif %}
                    </tr>
                    <tr>
                        <th style="text-align:left; width: 110px;">Tension </th>
                        <td style="width: 60px;">{{parametre.statorTension}} V</td>
                        {% if parametre.machine.categorie == "Asynchrone" or parametre.machine.categorie == "Synchrone" %}
                            <th style="text-align:left; width: 70px;">Fréquence</th>
                            <td> {{ parametre.statorFrequence }} Hz</td>
                        {% endif %}
                    </tr>

                    <tr>
                        <th style="text-align:left;">Courant</th>
                        <td>{{ parametre.statorCourant }} A</td>
                        {% if parametre.machine.categorie == "Asynchrone" or parametre.machine.categorie == "Synchrone" %}
                            <th style="text-align:left;">Couplage </th>
                            <td>{{ parametre.statorCouplage }}</td>
                        {% endif %}
                    </tr>

                    {% if parametre.statorTension2 != null or parametre.statorTension2 > 0 %}
                        <tr>
                            <th style="text-align:left;">Tension Excitation </th>
                            <td>{{parametre.statorTension2}} V</td>
                            {% if parametre.machine.categorie == "Asynchrone" or parametre.machine.categorie == "Synchrone" %}
                                <td></td>
                                <td></td>
                            {% endif %}
                        </tr>
                    {% endif %}

                    {% if parametre.statorCourantExcitation != null or parametre.statorCourantExcitation > 0 %}
                        <tr>
