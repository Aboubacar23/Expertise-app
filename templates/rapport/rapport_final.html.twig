    }

    .card-content h3 {
        font-size: 28px;
        margin: 0;
    }

    .card-content p {
        font-size: 14px;
        margin: 5px 0;
    }

    .table_matiere {
        padding-top: 200px;
    }

    .page_break {
        page-break-before: always;
    }

    .table_sous_page51 {
        width: 50%;
        float: left;
    }

    .table_sous_page52 {
        width: 50%;
        float: right;
    }

    .table_sous_page1,
    .table_sous_page5_6_1,
    .table_sous_page5_7_1 {
        width: 50%;
        float: left;
    }

    .table_sous_page2,
    .table_sous_page5_6_2,
    .table_sous_page5_7_2 {
        width: 50%;
        float: right;
    }

    .taille_cellulle80 {
        width: 80px;
    }

    .sous_page5_7 {
        padding-top: 120px;
    }

    .aligne_cellulle_left {
        text-align: left;
    }

    .sous_page3 {
        padding-top: 80px;
    }

    .table_sous_page3 {
        padding-top: 250px;
    }

    .numero {
        width: 100px;
    }

    .numero2 {
        width: 50px;
    }

    .sous_page {
        padding-bottom: 15px;
    }

    .sous_page2 {
        padding-bottom: 85px;
    }

    .sous_page4_1 {
        padding-bottom: 15px;
    }

    .sous_page4_2 {
        padding-bottom: 15px;
    }

    .table_sous_page_6_2,
    .tr_table_sous_page_6_2 {
        font-size: 11px !important;
    }

    .th_table_sous_page6_2 {
        width: 20% !important;
    }

    .td_image {
        text-align: center;
    }

    .sous_menu1 {
        padding-left: 20px;
        font-size: 14px;
    }

    .sous_menu2 {
        padding-left: 40px;
        font-size: 12px;
    }
    .table_signature{
        padding-top: 30px;
    }
</style>

<body class="body">
    {# la partie logo et titre #}
    <div class="logo">
        <img src="{{ absolute_url(asset('images/logo2.png')) }}" alt="" height="100px">
    </div>
    <div class="titre_rapport">
        <h1 class="titre1">Rapport Final</h1>
        <div class="card1">
            <div class="card-content">
                <h3>{{parametre.affaire.client.nom}}</h3>
                <h3>-</h3>
                <h3>Site : {{parametre.affaire.client.site}}</h3>
            </div>
        </div>
        <div class="card2">
            <div class="card-content2">
                <p>Affaire suivie par : <strong>{{parametre.affaire.suiviPar}}</strong></p>
                <p>Date : <strong>{{ "now" | date('d-m-Y') }}</strong></p>
                <p>Affaire JE N° : <strong>{{parametre.affaire.numAffaire}}</strong></p>
                <br>
                <p>Cde : <strong>{{parametre.affaire.codeClient}}</strong></p>
                {% if parametre.affaire.numFabrication != "" and parametre.affaire.numFabrication != "N/A" %}
                    <p>N° de fabrication : <strong>{{parametre.affaire.numFabrication}}</strong></p>
                {% endif %}
                {% if parametre.affaire.numArticleClient != "" and parametre.affaire.numArticleClient != "N/A" %}
                    <p>N° d’article client : <strong>{{parametre.affaire.numArticleClient}}</strong></p>
                {% endif %}
            </div>
        </div>

        {% if parametre.signature is not null %}
            <div class="table_signature">
                <table>
                    <tr class="titre_table" style="text-transform: uppercase">
                        <th style="width: 50px"></th>
                        <th>Expertise électrique avant lavage</th>
                        <th>Expertise électrique après lavage</th>
                        <th>Expertise Mécanique</th>
                        <th>Validation du rapport d'expertise</th>
                    </tr>
                    <tr style="text-align: center">
                        <td>Opérateur</td>
                        <td>
                            {% if parametre.signature.expAvantLavage == 1 %}
                                <strong>{{ parametre.signature.operateurExpAvantLavage.prenom }} {{ parametre.signature.operateurExpAvantLavage.nom }}</strong><br>
                                <p>{{ parametre.signature.dateExpAvantLavage ? parametre.signature.dateExpAvantLavage | date('d-m-Y') }}</p>
                                {% if parametre.signature.operateurExpAvantLavage.signaturePhoto %}
                                    <img src="{{ absolute_url(asset('signatures/'~parametre.signature.operateurExpAvantLavage.signaturePhoto)) }}" alt="" height="50px">
                                {% endif %}
                            {% endif %}
                        </td>
                        <td>
                            {% if parametre.signature.expApresLavage == 1 %}
                                <strong>{{ parametre.signature.operateurExpApresLavage.prenom }} {{ parametre.signature.operateurExpApresLavage.nom }}</strong><br>
                                <p>{{ parametre.signature.dateExpApresLavage ? parametre.signature.dateExpApresLavage | date('d-m-Y') }}</p>
                                {% if parametre.signature.operateurExpApresLavage.signaturePhoto %}
                                    <img src="{{ absolute_url(asset('signatures/'~parametre.signature.operateurExpApresLavage.signaturePhoto)) }}" alt="" height="50px">
                                {% endif %}
                            {% endif %}
                        </td>
                        <td>
                            {% if parametre.signature.expMeca == 1 %}
                                <strong>{{ parametre.signature.operateurExpMeca.prenom }} {{ parametre.signature.operateurExpMeca.nom }}</strong><br>
                                <p>{{ parametre.signature.dateExpAvantLavage ? parametre.signature.dateExpAvantLavage | date('d-m-Y') }}</p>
                                {% if parametre.signature.operateurExpMeca.signaturePhoto %}
                                    <img src="{{ absolute_url(asset('signatures/'~parametre.signature.operateurExpMeca.signaturePhoto)) }}" alt="" height="50px">
                                {% endif %}
                            {% endif %}
                        </td>
                        <td>
                            {% if parametre.signature.validationExp == 1 %}
                                <strong>{{ parametre.signature.operateurValidationExp.prenom }} {{ parametre.signature.operateurValidationExp.nom }}</strong><br>
                                <p>{{ parametre.signature.dateValidationExp ? parametre.signature.dateValidationExp | date('d-m-Y') }}</p>
                                {% if parametre.signature.operateurValidationExp.signaturePhoto %}
                                    <img src="{{ absolute_url(asset('signatures/'~parametre.signature.operateurValidationExp.signaturePhoto)) }}" alt="" height="50px">
                                {% endif %}
                            {% endif %}
                        </td>
                    </tr>
                </table>
                <table>
                    <tr class="titre_table" style="text-transform: uppercase">
                        <th style="width: 50px"></th>
                        <th>Remontage</th>
                        <th>Essais Finaux</th>
                        <th>Validation du rapport final</th>
                    </tr>
                    <tr style="text-align: center">
                        <td>Opérateur</td>
                        <td>
                            {% if parametre.signature.remontage == 1 %}
                                <strong>{{ parametre.signature.operateurRemontage.prenom }} {{ parametre.signature.operateurRemontage.nom }}</strong><br>
                                <p>{{ parametre.signature.dateRemontage ? parametre.signature.dateRemontage | date('d-m-Y') }}</p>
                                {% if parametre.signature.operateurRemontage.signaturePhoto %}
                                    <img src="{{ absolute_url(asset('signatures/'~parametre.signature.operateurRemontage.signaturePhoto)) }}" alt="" height="50px">
                                {% endif %}
                            {% endif %}
                        </td>
                        <td>
                            {% if parametre.signature.essaiFinaux == 1 %}
                                <strong>{{ parametre.signature.operateurEssaiFinaux.prenom }} {{ parametre.signature.operateurEssaiFinaux.nom }}</strong><br>
                                <p>{{ parametre.signature.dateEssaiFinaux ? parametre.signature.dateEssaiFinaux | date('d-m-Y') }}</p>
                                {% if parametre.signature.operateurEssaiFinaux.signaturePhoto %}
                                    <img src="{{ absolute_url(asset('signatures/'~parametre.signature.operateurEssaiFinaux.signaturePhoto)) }}" alt="" height="50px">
                                {% endif %}
                            {% endif %}
                        </td>
                        <td>
                            {% if parametre.signature.validationFinale == 1 %}
                                <strong>{{ parametre.signature.operateurValidationFinale.prenom }} {{ parametre.signature.operateurValidationFinale.nom }}</strong><br>
                                <p>{{ parametre.signature.dateValidationFinale ? parametre.signature.dateValidationFinale | date('d-m-Y') }}</p>
                                {% if parametre.signature.operateurValidationFinale.signaturePhoto %}
                                    <img src="{{ absolute_url(asset('signatures/'~parametre.signature.operateurValidationFinale.signaturePhoto)) }}" alt="" height="50px">
                                {% endif %}
                            {% endif %}
                        </td>
                    </tr>
                </table>
            </div>

        {% endif %}
    </div>
    {# fin de la partie logo et titre #}

    {# Sommaire #}
    <div class="page_break"></div>
    <div class="page2">
        <div>
            <h2>Sommaire</h2>
            <p><a href="#lien1">I. Préambule et Contexte</a></p>
            <p><a href="#lien2">II. Présentation des travaux</a></p>
            <p><a href="#lien3">III. Caractéristiques générales</a></p>
            <p><a href="#lien4">IV. Expertise à réception</a></p>
            <p class="sous_menu1"><a href="#lien41"> A. Contrôles et recensement</a></p>
            <p><a href="#lien5">V. Expertise mécanique</a></p>
            <p class="sous_menu1"><a href="#lien51"> A. Démontage</a></p>
            <p class="sous_menu2"><a href="#lien511"> </a></p>
            <p class="sous_menu2"><a href="#lien512"> 2.  Relevé boîte à borness</a></p>
            <p class="sous_menu2"><a href="#lien513"> 3. Synoptique de démontage</a></p>
            <p class="sous_menu1"><a href="#lien52"> B. Contrôle démontage : paliers</a></p>
            <p class="sous_menu1"><a href="#lien53"> C. Contrôle des coussinets</a></p>
            <p class="sous_menu2"><a href="#lien531"> 1. Expertise des soies</a></p>
            <p class="sous_menu2"><a href="#lien532"> 2. Relevés alésage coussinets</a></p>
            <p class="sous_menu2"><a href="#lien533"> 3. Expertise des coussinets</a></p>
            <p class="sous_menu1"><a href="#lien54"> D. Relevés dimensionnels rotor et paliers</a></p>
            <p class="sous_menu1"><a href="#lien55"> E. Contrôles géométriques du rotor</a></p>
            <p class="sous_menu1"><a href="#lien56"> F. Appareils de mesure utilisés</a></p>
            <p class="sous_menu1"><a href="#lien57"> G. Expertise Hydro-réfrigérant</a></p>
            <p class="sous_menu1"><a href="#lien58"> H. Expertise Aéroréfrigérant</a></p>
            <p class="sous_menu1"><a href="#lien59"> I. Illustration des observations</a></p>
            <p class="sous_menu1"><a href="#lien510"> J. Observations, constats et préconisations Mécaniques</a></p>
            <p><a href="#lien6">VI. Expertise électrique avant lavage</a></p>
            <p class="sous_menu1"><a href="#lien61"> A. Contrôle visuel et recensementontage</a></p>
            {% if parametre.essaisPlateforme == 'Oui' %}
            <p class="sous_menu1"><a href="#lien62"> B. Contrôles électriques à Réception</a></p>
            <p class="sous_menu2"><a href="#lien621"> 1. Mesure d’isolement</a></p>
            <p class="sous_menu2"><a href="#lien622"> 2. Mesure de Résistance </a></p>
            <p class="sous_menu2"><a href="#lien623"> 3. Points de fonctionnement à vide</a></p>
            <p class="sous_menu2"><a href="#lien624"> 4. Mesures de vibrations</a></p>
            <p class="sous_menu2"><a href="#lien631"> 5. Appareils de mesure utilisés</a></p>
            {% elseif parametre.essaisPlateforme == 'Non' %}
            <p class="sous_menu1"><a href="#lien63"> B. Contrôles électriques</a></p>
            <p class="sous_menu2"><a href="#lien621"> 1. Mesure d’isolement</a></p>
            <p class="sous_menu2"><a href="#lien622"> 2. Mesure de Résistance </a></p>
            <p class="sous_menu2"><a href="#lien631"> 3. Appareils de mesure utilisés</a></p>
            {% endif %}
            <p class="sous_menu1"><a href="#lien64"> C. Contrôles bobinage</a></p>
            <p class="sous_menu1"><a href="#lien65"> D. Balais & balais de masse</a></p>
            <p class="sous_menu1"><a href="#lien66"> E. Illustrations</a></p>
            <p class="sous_menu1"><a href="#lien67"> F. Observations, préconisations et conclusions ECE</a></p>
            <p><a href="#lien7">VII. Expertise électrique après lavage</a></p>
            <p class="sous_menu1"><a href="#lien71"> A. </a></p>
            <p class="sous_menu2"><a href="#lien711"> 1. Mesure d'isolement</a></p>
            <p class="sous_menu2"><a href="#lien712"> 2. Mesure de Résistances</a></p>
            {% if parametre.pontDiodes |length > 0 and parametre.equirepartitions |length > 0 %}
            <p class="sous_menu2"><a href="#lien712"> 3. Mesure d'équi-répartition de tension</a></p>
            <p class="sous_menu2"><a href="#lien712"> 4. Pont de Diodes</a></p>
            {% elseif parametre.equirepartitions |length > 0 %}
            <p class="sous_menu2"><a href="#lien712"> 3. Mesure d'équi-répartition de tension</a></p>
            {% elseif parametre.pontDiodes |length > 0 %}
            <p class="sous_menu2"><a href="#lien712"> 3. Pont de Diodes</a></p>
            {% endif %}
            {% if parametre.pontDiodes |length > 0 and parametre.equirepartitions |length > 0 %}
            <p class="sous_menu2"><a href="#lien713"> 5. Appareils de mesure utilisés</a></p>
            {% elseif parametre.pontDiodes |length > 0 or parametre.equirepartitions |length > 0 %}
            <p class="sous_menu2"><a href="#lien713"> 4. Appareils de mesure utilisés</a></p>
            {% else %}
            <p class="sous_menu2"><a href="#lien713"> 3. Appareils de mesure utilisés</a></p>
            {% endif %}
            <p class="sous_menu1"><a href="#lien72"> B. Observations, préconisations et conclusions ECE</a></p>
            <p><a href="#lien8">VIII. Contre-Expertise</a></p>
            <p><a href="#lien10">IX. Remontage</a></p>
            <p class="sous_menu1"><a href="#lien103"> A. Equilibrage</a></p>
            <p class="sous_menu1"><a href="#lien101"> B. Remontage</a></p>
            <p class="sous_menu1"><a href="#lien102"> C. Remontage paliers</a></p>
            <p class="sous_menu1"><a href="#lien104"> D. Photos Remontage</a></p>
            <p><a href="#lien11">X. Essais en rotation</a></p>
            <p class="sous_menu1"><a href="#lien111"> A. Remontage</a></p>
            <p class="sous_menu2"><a href="#lien1111"> 1. Mesure d’isolement et Index</a></p>
            <p class="sous_menu2"><a href="#lien1112"> 2. Mesure des résistances ohmiques</a></p>
            <p class="sous_menu2"><a href="#lien1113"> 3. Point de fonctionnement à vide</a></p>
            <p class="sous_menu2"><a href="#lien1114"> 4. Carctérisques à vide</a></p>
            <p class="sous_menu2"><a href="#lien1115"> 5. Mesures de vibrations</a></p>
            <p class="sous_menu2"><a href="#lien1116"> 6. Point de fonctionnement rotor bloqué</a></p>
